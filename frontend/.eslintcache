ER_FAIL,\n  USER_DETAILS_REQUEST,\n  USER_DETAILS_SUCCESS,\n  USER_DETAILS_FAIL,\n  USER_UPDATE_PROFILE_REQUEST,\n  USER_UPDATE_PROFILE_SUCCESS,\n  USER_UPDATE_PROFILE_FAIL,\n  USER_UPDATE_PROFILE_RESET,\n} from \"../constants/userConstants\";\n\nexport const login = (email, password) => async (dispatch) => {\n  try {\n    // Dispatch request action\n    dispatch({\n      type: USER_LOGIN_REQUEST,\n    });\n\n    // Send content type in the header\n    const config = {\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n    };\n\n    // Make login request and destructure the data\n    const { data } = await axios.post(\n      \"/api/users/login\",\n      { email, password },\n      config\n    );\n\n    // Dispatch the success login action\n    dispatch({\n      type: USER_LOGIN_SUCCESS,\n      payload: data,\n    });\n\n    // Save user data to local storage\n    localStorage.setItem(\"userInfo\", JSON.stringify(data));\n  } catch (error) {\n    // Failed to login\n    dispatch({\n      type: USER_LOGIN_FAIL,\n      // First check for the error message in the backend\n      payload:\n        error.response && error.response.data.message\n          ? error.response.data.message\n          : error.message,\n    });\n  }\n};\n\nexport const logout = () => async (dispatch) => {\n  // Remove user from localStorage\n  localStorage.removeItem(\"userInfo\");\n\n  dispatch({\n    type: USER_LOGOUT,\n  });\n};\n\nexport const register = (name, email, password) => async (dispatch) => {\n  try {\n    // Dispatch request action\n    dispatch({\n      type: USER_REGISTER_REQUEST,\n    });\n\n    // Send content type in the header\n    const config = {\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n    };\n\n    // Make register request and destructure the data\n    const { data } = await axios.post(\n      \"/api/users\",\n      { name, email, password },\n      config\n    );\n\n    // Dispatch the success register action\n    dispatch({\n      type: USER_REGISTER_SUCCESS,\n      payload: data,\n    });\n\n    // Auto login after a successful registeration\n    dispatch({\n      type: USER_LOGIN_SUCCESS,\n      payload: data,\n    });\n\n    // Save user data to local storage\n    localStorage.setItem(\"userInfo\", JSON.stringify(data));\n  } catch (error) {\n    // Failed to login\n    dispatch({\n      type: USER_REGISTER_FAIL,\n      // First check for the error message in the backend\n      payload:\n        error.response && error.response.data.message\n          ? error.response.data.message\n          : error.message,\n    });\n  }\n};\n\n// Get user Profile\nexport const getUserDetails = (id) => async (dispatch, getState) => {\n  try {\n    // Dispatch request action\n    dispatch({\n      type: USER_DETAILS_REQUEST,\n    });\n\n    // Get user token from state (from logged in user object)\n    const {\n      userLogin: { userInfo },\n    } = getState();\n\n    // Send content type and the  token in the headers\n    const config = {\n      headers: {\n        \"Content-Type\": \"application/json\",\n        Authorization: `Bearer ${userInfo.token}`,\n      },\n    };\n\n    // Make get profile request and destructure the data\n    const { data } = await axios.get(`/api/users/${id}`, config);\n\n    // Dispatch the success details action\n    dispatch({\n      type: USER_DETAILS_SUCCESS,\n      payload: data,\n    });\n  } catch (error) {\n    // Failed to login\n    dispatch({\n      type: USER_DETAILS_FAIL,\n      // First check for the error message in the backend\n      payload:\n        error.response && error.response.data.message\n          ? error.response.data.message\n          : error.message,\n    });\n  }\n};\n\n// Update user Profile\nexport const updateUserProfile = (user) => async (dispatch, getState) => {\n  try {\n    // Dispatch request action\n    dispatch({\n      type: USER_UPDATE_PROFILE_REQUEST,\n    });\n\n    // Get user info from state (logged in user object)\n    const {\n      userLogin: { userInfo },\n    } = getState();\n\n    // Send content type and the  token in the headers\n    const config = {\n      headers: {\n        \"Content-Type\": \"application/json\",\n        Authorization: `Bearer ${userInfo.token}`,\n      },\n    };\n\n    // Make put request and destructure the data after updating\n    const { data } = await axios.put(`/api/users/profile`, user, config);\n\n    // Dispatch the success update action\n    dispatch({\n      type: USER_UPDATE_PROFILE_SUCCESS,\n      payload: data,\n    });\n  } catch (error) {\n    // Failed to login\n    dispatch({\n      type: USER_UPDATE_PROFILE_FAIL,\n      // First check for the error message in the backend\n      payload:\n        error.response && error.response.data.message\n          ? error.response.data.message\n          : error.message,\n    });\n  }\n};\n","/home/mohamed/Node-js/shopilla/frontend/src/screens/RegisterScreen.js",[],"/home/mohamed/Node-js/shopilla/frontend/src/screens/ProfileScreen.js",[],"/home/mohamed/Node-js/shopilla/frontend/src/screens/ShippingScreen.js",[],"/home/mohamed/Node-js/shopilla/frontend/src/components/CheckoutSteps.js",[],"/home/mohamed/Node-js/shopilla/frontend/src/screens/PaymentScreen.js",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":16,"column":3,"nodeType":"129","messageId":"130","endLine":16,"endColumn":28},"no-native-reassign",["131"],"no-negated-in-lhs",["132"],"no-unused-vars","'USER_UPDATE_PROFILE_RESET' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]